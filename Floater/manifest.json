{
  "name": "Floater",
  "manifest_version": 2,
  "version": "0.1",
  "content_scripts": [{
    "matches": [ "<all_urls>" ],
    "js": [ "jquery.js", "jqfloat.js", "content.js" ]
  }],
  "browser_action": {
    "default_icon": "icon-float.png"
  },
  "background": {
    "scripts": ["background.js"]
  }
}
